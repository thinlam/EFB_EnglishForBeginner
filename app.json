{
  "expo": {
    "name": "EFB - English For Beginners",
    "slug": "MyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_1024x1024.png",
    "scheme": "com.efb.userapp",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/images/splash_1242x2436.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFE3B0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.efb.userapp",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFE3B0"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#FFE3B0"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "router": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "92a684ce-be18-442e-a388-2670d0cb5629"
      }
    }
  }
}
